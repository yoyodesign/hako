@require "headings"
@require "h6"
@require "h5"
@require "h4"

if $hako["use"]["typography"]["headings"] is true
	if "h3" in $hako["typography"]
		main h3,
		.gamma
			hako-typography($hako["typography"]["h3"])

	else if "gamma" in $hako["typography"]
		.gamma
			hako-typography($hako["typography"]["gamma"])
